<Application x:Class="_06._WpfExercise_v._2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:_06._WpfExercise_v._2"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <RadialGradientBrush RadiusX="0.1" RadiusY="0.1" x:Key="BlurryEyes" SpreadMethod="Reflect">
            <GradientStop Offset="0" Color="GreenYellow" />
            <GradientStop Offset="1" Color="HotPink" />
        </RadialGradientBrush>
    </Application.Resources>
</Application>
